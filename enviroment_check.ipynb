   "source": [
    "import qiskit as q\n",
    "%matplotlib inline\n",
    "\n",
    "circuit = q.QuantumCircuit(2,2) # 2 qbits , 2 classical bit\n",
    "# Currently 0,0\n",
    "\n",
    "circuit.x(0)\n",
    "# 1,0\n",
    "circuit.cx(0,1) # cnot, controlled not flips 2nd qbit value IF first qbit is 1\n",
    " # 1, 1\n",
    "circuit.measure([0,1], [0,1]) \n",
    "circuit.draw()\n"
   ]
